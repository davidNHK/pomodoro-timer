    "collectCoverageFrom": [
      "<rootDir>/**/*.ts",
      "!<rootDir>/**/*.spec.ts",
      "!<rootDir>/migrations/*.ts"
    ],
    "coverageDirectory": "<rootDir>/../coverage",
    "injectGlobals": false,
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  },
