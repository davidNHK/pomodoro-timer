  "name": "backend",
  "private": true,
  "scripts": {
    "build": "nest build",
    "eslint": "eslint --ext=json,ts,yml",
    "lint": "npx eslint --ext=json,ts,yml --fix .",
    "lint:ci": "npx eslint --ext=json,ts,yml .",
    "prebuild": "rimraf dist",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main.js",
    "test": "jest --maxWorkers=50%",
    "test:ci": "APP_ENV=test NODE_ENV=test Ci=true jest --coverage --ci --runInBand --bail"
